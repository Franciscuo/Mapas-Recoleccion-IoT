                                        <option value="15">Antonio Nari침o</option>
                                        <option value="16">Puente Aranda</option>
                                        <option value="17">La Candelaria</option>
                                        <option value="18">Rafael Uribe Uribe</option>
                                        <option value="19">Ciudad Bolivar</option>
                                        <option value="20">Sumapaz</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <input type="number" step="0.00001" min="180" max="-180" name="latitude" id="lat" class="form-control"
                                        placeholder="Latitud" autocomplete="off" required="true">
                                    <div class="invalid-feedback">
                                        La latitud es invalida
                                    </div>
                                </div>

                                <div class="form-group">
                                    <input type="number" step="0.00001" min="180" max="-180" name="longitude" id="lon" class="form-control"
                                        placeholder="Longitud" autocomplete="off" required="true">
                                    <div class="invalid-feedback">
                                        La longitud es invalida
                                    </div>
                                </div>
                                <div class="form-group">
                                    <button id="btnSend" class="btn btn-success btn-block">A침adir Nodo</button>
                                </div>
                            {{!-- </form> --}}
                        </div>
                    </div>
                </div>
        </section>
    </div>
</div>
